{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "wesh dev --use app bin",
      "type": "dart",
      "request": "launch",
      "args": [
        "--flavor",
        "dev",
        "--use-application-binary",
        "build/app/outputs/apk/dev/debug/app-dev-debug.apk"
      ]
    },
    {
      "name": "wesh dev (debug)",
      "type": "dart",
      "request": "launch",
      "flutterMode": "debug",
      "args": ["--flavor", "dev"]
    },
    {
      "name": "wesh dev (profile)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": ["--flavor", "dev"]
    },
    {
      "name": "wesh dev (release)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["--flavor", "dev"]
    },

    {
      "name": "wesh (debug)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": ["--flavor", "prod"]
    }
  ]
}
