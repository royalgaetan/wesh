  "version": "0.2.0",
  "configurations": [
    {
      "name": "wesh --use app bin",
      "type": "dart",
      "request": "launch",
      "args": [
        "--use-application-binary",
        "build/app/outputs/apk/debug/app-debug.apk"
      ]
    },

    {
      "name": "wesh",
      "request": "launch",
      "type": "dart"
    },

    {
      "name": "wesh (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "wesh (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
